<div class="clearfix">
  <div class="col-md-6">
    <h2>Contribs <a href="<%- contribsPath %>.xml" class="icon icon-rss"></a></h2>
    <a class="btn btn-primary btn-lg" href="/contribs/add">Submit a contribution</a>
    <% if (contribs) { %>
    <% for (var i = 0, ii = contribs.length; i < ii; i++) { %>
      <div class="">
        <h3><%- linkTo(contribs[i].title, contribPath(contribs[i].id)); %></h3>
      </div>
    <% } %>
    <% } %>
  </div>

  <div class="col-md-6">
    <h2>Interests <a href="<%- interestsPath %>.xml" class="icon icon-rss"></a></h2>
    <a class="btn btn-primary btn-lg" href="/interests/add">Submit an interest</a>
    <% if (interests) { %>
    <% for (var i = 0, ii = interests.length; i < ii; i++) { %>
      <div class="">
        <h3><%- linkTo(interests[i].title, interestPath(interests[i].id)); %></h3>
      </div>
    <% } %>
    <% } %>
  </div>
</div>
