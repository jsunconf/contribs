<form id="contrib-form" class="form-horizontal" action="/contribs" method="POST">
  <fieldset>
    <legend>Create a new Contrib</legend>
    <% if(params.errors) { %>
    <div class="control-group">
      <ul>
      <% for(var err in params.errors) { %>
        <li><%= params.errors[err]; %></li>
      <% } %>
      </ul>
    </div>
    <% } %>

    <%- partial('form', {contrib: {}}) %>

    <%- scriptLink('/js/recaptcha.js', {type: 'text/javascript'}) %>
    <%- recaptcha %>

    <div class="form-actions">
      <%- contentTag('input', 'Add', {type: 'submit', class: 'btn btn-primary'}) %>
    </div>
  </fieldset>
</form>


